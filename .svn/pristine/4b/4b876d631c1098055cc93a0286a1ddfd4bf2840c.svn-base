package com.chinasofti.eec.day002.homework;

public class Di_4_v2 {

	public static void main(String[] args) {
		// 三个数的排序
		int a = 58, b = 32, c = 87;
		if (a > b) { // 确保b>a
			int t = a;
			a = b;
			b = t;
		}
		if (a > c) { // 确保c>a
			int t = a;
			a = c;
			c = t;
		}
		if (b > c) { // 确保c>b
			int t = b;
			b = c;
			c = t;
		}

		System.out.println("a = " + a + "  b = " + b + "  c = " + c);
	}

}
